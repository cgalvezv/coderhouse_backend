{
  "name": "coderhouse_backend",
  "version": "0.0.0",
  "description": "Desafíos y proyectos para el curso Programación Backend - Coderhouse",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "migrate": "node ./src/database/migration",
    "drop": "node ./src/database/drop",
    "test": "test",
    "forever_start": "forever start server.js",
    "forever_stop": "forever stop 0",
    "pm2_fork": "pm2 start server.js --name='CoderhouseServerFork' --watch -- PORT=8081",
    "pm2_cluster": "pm2 start server.js --name='CoderhouseServerCluster' --watch -i max -- PORT=8082",
    "pm2_stopall": "pm2 delete CoderhouseFork && pm2 delete CoderhouseCluster"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cgalvezv/coderhouse_backend.git"
  },
  "keywords": [
    "Coderhouse",
    "Programación",
    "Backend"
  ],
  "author": "Camilo Gálvez Vidal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cgalvezv/coderhouse_backend/issues"
  },
  "homepage": "https://github.com/cgalvezv/coderhouse_backend#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "connect-mongo": "^4.4.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.2",
    "express-session": "^1.17.2",
    "faker": "^5.5.3",
    "mongoose": "^5.13.2",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "normalizr": "^3.6.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "socket.io": "^4.1.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
